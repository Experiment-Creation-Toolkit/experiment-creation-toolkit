{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "id",
        "type"
    ],
    "additionalProperties": false,
    "properties": {
        "$schema": {
            "type": "string",
            "format": "uri-reference"
        },
        "id": {
            "type": "string"
        },
        "type": {
            "type": "string",
            "enum": [
                "text",
                "prototype",
                "input"
            ]
        },
        "properties": {
            "type": "object"
        }
    }
}